{
  "name": "chore-cat",
  "version": "0.4.0",
  "description": "helper for managing chores around the house",
  "main": "src/index.js",
  "scripts": {
    "test": "karma start karma.conf.js --single-run",
    "test:watch": "karma start karma.conf.js",
    "serve": "live-server build",
    "build": "browserify -g uglifyify $npm_package_main -o build/bundle.js",
    "watch": "watchify $npm_package_main -d -o build/bundle.js",
    "start": "parallelshell 'npm run watch -s' 'npm run serve -s' 'npm run test:watch'"
  },
  "keywords": [
    "chores",
    "cats"
  ],
  "author": "Ryan Davis <ryepup@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babelify": "^6.4.0",
    "browserify": "^11.0.1",
    "browserify-ngannotate": "^1.0.1",
    "cssify": "^0.7.0",
    "envify": "^3.4.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.0",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "live-server": "^0.8.1",
    "parallelshell": "^2.0.0",
    "phantomjs": "^1.9.18",
    "stringify": "^3.1.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "angular": "^1.4.5",
    "angular-bootstrap": "^0.12.2",
    "angular-qrcode": "^6.0.2",
    "angular-smart-table": "^2.1.3",
    "angular-ui-router": "^0.2.15",
    "firebase": "^2.2.9",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "qrcode-generator": "^1.0.0",
    "uuid": "^2.0.1"
  },
  "browserify": {
    "transform": [
      "cssify",
      "stringify",
      "envify",
      "browserify-ngannotate",
      "babelify",
      "uglifyify"
    ]
  }
}
