    "cats"
  ],
  "author": "Ryan Davis <ryepup@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^11.0.1",
    "cssify": "^0.7.0",
    "envify": "^3.4.0",
    "live-server": "^0.8.1",
    "parallelshell": "^2.0.0",
    "stringify": "^3.1.0",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "angular": "^1.4.5",
    "angular-bootstrap": "^0.12.2",
    "angular-qrcode": "^6.0.2",
    "angular-smart-table": "^2.1.3",
    "angular-ui-router": "^0.2.15",
    "firebase": "^2.2.9",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "qrcode-generator": "^1.0.0",
    "uuid": "^2.0.1"
  },
  "browserify": {
    "transform": [
