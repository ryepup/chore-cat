{
  "name": "chore-cat",
  "version": "0.1.1",
  "description": "helper for managing chores around the house",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "live-server build",
    "build": "browserify $npm_package_main -o build/bundle.min.js",
